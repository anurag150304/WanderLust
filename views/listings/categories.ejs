                            <img src="https://a0.muscache.com/pictures/c5a4f6fc-c92c-4ae8-87dd-57f1ff1b89a6.jpg"
                                alt="png">
                        </div>
                        <p>OMG!</p>
                    </a>
                </li>
                <li><a href="/listings/category=Luxurious">
                        <div>
                            <img src="https://a0.muscache.com/pictures/c8e2ed05-c666-47b6-99fc-4cb6edcde6b4.jpg"
                                alt="png">
                        </div>
                        <p>Luxe</p>
                    </a>
                </li>
            </ul>
        </nav>
        <% if (DATA.length) { %>
            <div class="row row-cols-lg-4 row-cols-md-3 row-cols-sm-2 m-1 p-0">
                <% for(const val of DATA) { %>
                    <div class="card idx-card col m-3 p-0" style="width: 15.98rem;">
                        <a href="/listings/id=<%= val._id %>">
                            <img src="<%= val.image.url %>" class="card-img-top" style="height: 15.98rem;">
                            <div class="card-body m-0 p-0">
                                <ul class="list-group list-group-flush">
                                    <li class="list-group-item">
                                        <b>
                                            <%= val.title %>
                                        </b>
                                    </li>
                                    <li class="list-group-item">
                                        <b>&#8377;<%= val.price.toLocaleString("en-IN") %>/night</b>
                                    </li>
                                </ul>
                            </div>
                        </a>
                    </div>
                    <% } %>
            </div>
            <% } else { %>
                <div class="category-box">
                    <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5"
                        stroke="currentColor" class="icon">
                        <path stroke-linecap="round" stroke-linejoin="round"
                            d="M12 9v3.75m0 3.75h.007v-.008H12v.008zm9-9.75a9 9 0 11-18 0 9 9 0 0118 0z" />
                    </svg>
                    <span>Current category not found</span>
                </div>
                <% } %>

                    <footer class="fixed-bottom">
                        <div class="left">
